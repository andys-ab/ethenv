{
  "name": "ethenv",
  "version": "0.0.1",
  "description": "Cross-connectivity environment for javascript Ethereum clients.",
  "main": "dist/index.js",
  "repository": "git@github.com:nataouze/ethenv.git",
  "author": "Nathan Sala",
  "license": "MIT",
  "scripts": {
    "lint:fix": "prettier --write src/**/*.ts test/**/*.ts && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "prettier": "prettier --write src",
    "test": "mocha --require ts-node/register --require source-map-support/register --timeout 20000 --recursive test/**/*.test.ts --exit",
    "build": "tsc",
    "watch": "tsc -w",
    "prepack": "npm run lint:fix && npm run test && npm run build",
    "publish:next": "npm publish --tag next",
    "publish:release": "npm publish"
  },
  "dependencies": {
    "web3": "1.0.0-beta.55",
    "web3-core": "1.0.0-beta.55",
    "web3-eth-contract": "1.0.0-beta.55",
    "web3-providers": "^1.0.0-beta.55",
    "web3-utils": "1.0.0-beta.55"
  },
  "optionalDependencies": {
    "cross-fetch": "^3.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.0.1",
    "mochawesome": "^6.1.1",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  }
}
